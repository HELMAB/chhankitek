{
    "name": "asorasoft/chhankitek",
    "description": "Can display the Khmer calendar for different years",
    "keywords": [
        "asorasoft",
        "chhankitek"
    ],
    "homepage": "https://github.com/asorasoft/chhankitek",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mab Hel",
            "email": "mabhelitc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Asorasoft\\Chhankitek\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asorasoft\\Chhankitek\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asorasoft\\Chhankitek\\ChhankitekServiceProvider"
            ],
            "aliases": {
                "Chhankitek": "Asorasoft\\Chhankitek\\ChhankitekFacade"
            }
        }
    }
}
